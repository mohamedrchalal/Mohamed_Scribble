<h2>New Post</h2>

<%= form_for @post do |f| %>
  <%= f.label :title %>
  <br>
  <%= f.text_field :title %>
  <br>
  <br>
  <%= f.label :content %>
  <br>
  <%= f.text_area :content %>
  <br>
  <br>
  <%= f.submit %>
<% end %>


<%= form_for @post, method: :delete do |f| %>
  <%= f.submit "Delete!", class: "delete" %>
<% end %>
